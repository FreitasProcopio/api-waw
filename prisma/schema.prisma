// Prisma schema reflecting the Palavras model from main.model.js
// Ajuste os tipos conforme necess√°rio para o seu banco de dados real.

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Palavras {
    id       Int     @id @default(autoincrement())
    imagem   String? // Opcional
    people   String
    language String
    contexto String
    type     String
    caracter String?
}
